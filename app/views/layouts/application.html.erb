<!DOCTYPE html>
<html>
  <head>
    <title>SetList</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |type,message| %>
      <div class = <%= type %> >
        <p><%= message %></p>
      </div>
    <% end %>
    <%= yield %>
  </body>
</html>
